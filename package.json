{
	"name": "Rodan",
	"version": "1.0.0",
	"private": true,
	"author": "Caleb Depatie",
	"description": "Front End interface for SP",
	"main": "electron.js",
	"scripts": {
		"dev": "yarn run build:dev && yarn run start",
		"build:dev": "concurrently \"yarn run build:dev:main\" \"yarn run build:dev:renderer\"",
		"build:dev:main": "webpack --config ./webpack.config.main.js --mode=development",
		"build:dev:renderer": "webpack --config ./webpack.config.renderer.dev.js --mode=development",
		"build:prod": "concurrently \"yarn run build:prod:main\" \"yarn run build:prod:renderer\"",
		"build:prod:main": "webpack --config ./webpack.config.main.js --mode=production",
		"build:prod:renderer": "webpack --config ./webpack.config.renderer.prod.js --mode=production",
		"package": "yarn run build:prod && electron-builder",
		"start": "concurrently \"yarn run start:main\" \"yarn run start:renderer\"",
		"start:main": "electron build/electron.js",
		"start:renderer": "webpack serve --config ./webpack.config.renderer.dev.js --mode=development"
	},
	"build": {
		"asar": false,
		"icon": "./src/renderer/Public/favicon.ico",
		"files": [
			"build/*",
      "common/*"
		],
    "extraFiles": [
      "*.env"
    ],
		"win": {
			"target": "nsis"
		}
	},
	"dependencies": {
		"@fortawesome/fontawesome-free": "^5.15.4",
		"@popperjs/core": "^2.11.5",
		"@types/bootstrap": "^5.1.9",
		"@types/jest": "^27.4.0",
		"@types/node": "^17.0.13",
		"@types/react": "17.0.43",
		"@types/react-bootstrap": "^0.32.26",
		"@types/react-dom": "^17.0.14",
		"@types/react-router-dom": "^5.3.3",
		"bootstrap": "^5.1.3",
		"chart.js": "^3.5.0",
		"core-js": "^3.21.1",
		"dotenv": "^16.0.0",
		"electron-fetch": "^1.7.4",
		"path-browserify": "^1.0.1",
		"primeflex": "^3.0.0",
		"primeicons": "^5.0.0",
		"primereact": "^6.6.0",
		"react-bootstrap": "^2.0.2",
		"react-markdown": "^7.1.2",
		"react-popper": "^2.2.5",
		"react-router-dom": "^5.3.0",
		"react-toastify": "^8.2.0",
		"react-transition-group": "^4.4.2",
		"source-map-support": "^0.5.21",
		"terser-webpack-plugin": "^5.3.1"
	},
	"peerDependencies": {},
	"devDependencies": {
		"@babel/core": "^7.16.7",
		"@babel/plugin-syntax-flow": "^7.16.7",
		"@babel/plugin-transform-react-jsx": "^7.17.3",
		"@babel/plugin-transform-runtime": "^7.17.0",
		"@babel/preset-react": "^7.16.7",
		"@babel/preset-typescript": "^7.16.7",
		"@webpack-cli/serve": "^1.6.1",
		"concurrently": "^7.1.0",
		"copy-webpack-plugin": "^10.2.4",
		"css-loader": "^6.2.0",
		"electron": "^18.0.4",
		"electron-builder": "^23.0.3",
		"electron-webpack": "^2.8.2",
		"electron-webpack-ts": "^4.0.1",
		"html-webpack-plugin": "^5.5.0",
		"loader-utils": "^3.2.0",
		"node-sass": "^7.0.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-scripts": "^5.0.1",
		"sass": "^1.26.5",
		"sass-loader": "^12.6.0",
		"style-loader": "^3.3.1",
		"style-resources-loader": "^1.4.1",
		"ts-loader": "~8.2.0",
		"typescript": "^4.6.3",
		"webpack": "^5.72.0",
		"webpack-cli": "^4.9.2",
		"webpack-dev-server": "^4.8.1",
		"webpack-merge": "^5.8.0"
	}
}
