{
	"name": "Rodan",
	"version": "1.2.5",
	"private": true,
	"author": "Caleb Depatie",
	"description": "Front End interface for SP",
	"main": "electron.js",
	"scripts": {
		"update": "git pull && yarn",
		"dev": "yarn build:dev && yarn start",
		"build:dev": "conc \"yarn build:dev:main\" \"yarn build:dev:renderer\"",
		"build:dev:main": "webpack --config ./webpack.config.main.js --mode=development",
		"build:dev:renderer": "webpack --config ./webpack.config.renderer.dev.js --mode=development",
		"build:prod": "conc \"yarn build:prod:main\" \"yarn build:prod:renderer\"",
		"build:prod:main": "webpack --config ./webpack.config.main.js --mode=production",
		"build:prod:renderer": "webpack --config ./webpack.config.renderer.prod.js --mode=production",
		"package": "yarn build:prod && electron-builder",
		"start": "conc \"yarn start:main\" \"yarn start:renderer\"",
		"start:main": "electron build/electron.js",
		"start:renderer": "webpack serve --config ./webpack.config.renderer.dev.js --mode=development"
	},
	"build": {
		"asar": true,
		"files": [
			"build/*",
			"common/*"
		],
		"extraFiles": [
			"*.env",
			"*.node",
			"*.pdb"
		],
		"win": {
			"icon": "./src/renderer/Public/favicon.ico",
			"target": "nsis"
		},
		"linux": {
			"icon": "./src/static/icon.png",
			"target": "AppImage",
			"category": "Office"
		}
	},
	"dependencies": {
		"@fortawesome/fontawesome-free": "^6.3.0",
		"@popperjs/core": "^2.11.6",
		"@types/node": "^18.11.18",
		"@types/react": "^18.0.28",
		"@types/react-dom": "^18.0.10",
		"@types/react-router-dom": "^5.3.3",
		"chart.js": "^4.0.1",
		"dayjs": "^1.11.5",
		"dotenv": "^16.0.3",
		"electron-fetch": "^1.9.1",
		"node-ssh-forward": "^0.7.2",
		"path-browserify": "^1.0.1",
		"primeflex": "^3.2.1",
		"primeicons": "^5.0.0",
		"primereact": "^9.2.2",
		"react-chartjs-2": "^4.3.1",
		"react-markdown": "^8.0.5",
		"react-popper": "^2.3.0",
		"react-router-dom": "^6.4.1",
		"react-toastify": "^9.1.1",
		"react-transition-group": "^4.4.5"
	},
	"peerDependencies": {},
	"devDependencies": {
		"@babel/core": "^7.20.12",
		"@babel/plugin-syntax-flow": "^7.18.6",
		"@babel/plugin-transform-react-jsx": "^7.20.7",
		"@babel/plugin-transform-runtime": "^7.21.0",
		"@babel/preset-react": "^7.18.6",
		"@babel/preset-typescript": "^7.18.6",
		"@webpack-cli/serve": "^1.7.0",
		"concurrently": "^7.6.0",
		"copy-webpack-plugin": "^11.0.0",
		"css-loader": "^6.7.3",
		"electron": "^22.1.0",
		"electron-builder": "^23.3.3",
		"electron-webpack": "^2.8.2",
		"electron-webpack-ts": "^4.0.1",
		"html-webpack-plugin": "^5.5.0",
		"loader-utils": "^3.2.1",
		"node-sass": "^8.0.0",
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"react-scripts": "^5.0.1",
		"sass": "^1.55.0",
		"sass-loader": "^13.2.0",
		"source-map-support": "^0.5.21",
		"style-loader": "^3.3.1",
		"style-resources-loader": "^1.4.1",
		"terser-webpack-plugin": "^5.3.6",
		"ts-loader": "~9.4.2",
		"typescript": "^4.9.4",
		"webpack": "^5.75.0",
		"webpack-cli": "^4.10.0",
		"webpack-dev-server": "^4.11.1",
		"webpack-merge": "^5.8.0"
	}
}
