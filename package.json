{
	"name": "Rodan",
	"version": "1.1.1",
	"private": true,
	"author": "Caleb Depatie",
	"description": "Front End interface for SP",
	"main": "electron.js",
	"scripts": {
		"dev": "yarn build:dev && yarn start",
		"build:dev": "yarn build:zig && yarn build:dev:main && yarn run build:dev:renderer",
		"build:dev:main": "webpack --config ./webpack.config.main.js --mode=development",
		"build:dev:renderer": "webpack --config ./webpack.config.renderer.dev.js --mode=development",
		"build:prod": "yarn build:zig && yarn build:prod:main && yarn build:prod:renderer",
		"build:prod:main": "webpack --config ./webpack.config.main.js --mode=production",
		"build:prod:renderer": "webpack --config ./webpack.config.renderer.prod.js --mode=production",
		"build:zig": "zig build",
		"package": "yarn build:prod && electron-builder",
		"start": "concurrently \"yarn start:main\" \"yarn start:renderer\"",
		"start:main": "electron build/electron.js",
		"start:renderer": "webpack serve --config ./webpack.config.renderer.dev.js --mode=development"
	},
	"build": {
		"asar": true,
		"icon": "./src/renderer/Public/favicon.ico",
		"files": [
			"build/*",
			"common/*"
		],
		"extraFiles": [
			"*.env",
      "*.node",
      "*.pdb"
		],
		"win": {
			"target": "nsis"
		}
	},
	"dependencies": {
		"@fortawesome/fontawesome-free": "^6.1.2",
		"@popperjs/core": "^2.11.5",
		"@types/node": "^17.0.13",
		"@types/react": "^18.0.0",
		"@types/react-dom": "^18.0.0",
		"@types/react-router-dom": "^5.3.3",
		"dayjs": "^1.11.4",
		"dotenv": "^16.0.0",
		"electron-fetch": "^1.7.4",
		"path-browserify": "^1.0.1",
		"primeflex": "^3.0.0",
		"primeicons": "^5.0.0",
		"primereact": "^8.1.1",
		"react-markdown": "^7.1.2",
		"react-popper": "^2.3.0",
		"react-router-dom": "^6.3.0",
		"react-toastify": "^8.2.0",
		"react-transition-group": "^4.4.2",
		"source-map-support": "^0.5.21",
		"terser-webpack-plugin": "^5.3.1"
	},
	"peerDependencies": {},
	"devDependencies": {
		"@babel/core": "^7.18.10",
		"@babel/plugin-syntax-flow": "^7.18.6",
		"@babel/plugin-transform-react-jsx": "^7.17.3",
		"@babel/plugin-transform-runtime": "^7.17.0",
		"@babel/preset-react": "^7.16.7",
		"@babel/preset-typescript": "^7.16.7",
		"@webpack-cli/serve": "^1.6.1",
		"concurrently": "^7.1.0",
		"copy-webpack-plugin": "^10.2.4",
		"css-loader": "^6.2.0",
		"electron": "^19.0.0",
		"electron-builder": "^23.0.3",
		"electron-webpack": "^2.8.2",
		"electron-webpack-ts": "^4.0.1",
		"html-webpack-plugin": "^5.5.0",
		"loader-utils": "^3.2.0",
		"node-sass": "^7.0.1",
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"react-scripts": "^5.0.1",
		"sass": "^1.26.5",
		"sass-loader": "^12.6.0",
		"style-loader": "^3.3.1",
		"style-resources-loader": "^1.4.1",
		"ts-loader": "~8.2.0",
		"typescript": "^4.6.3",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0",
		"webpack-dev-server": "^4.9.3",
		"webpack-merge": "^5.8.0"
	}
}
